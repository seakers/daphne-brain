{
  "type": "run_function",
  "params":
  [
    { "name": "edl_mat_param", "type": "edl_mat_param", "options": "", "mandatory": true,"from_context":false},
    { "name": "edl_mat_param2", "type": "edl_mat_param", "options": "", "mandatory": true,"from_context":false},
    { "name": "current_mat_file", "type": "edl_mat_file", "options": "", "from_context": true, "mandatory": true },
    { "name": "current_mat_file_for_print", "type": "edl_mat_file", "options": "", "from_context": true, "mandatory": true }
  ],
"function":
{"run_template": "run_func.edl_plot_from_matfile('${current_mat_file}','${edl_mat_param}', '${edl_mat_param2}',context)",
    "result_type": "list",
    "result_fields": {
     "load_result": "row.command_result"
    }
  },
  "voice_response": {
    "type": "list",
    "begin": "test",
    "repeat": "${load_result}",
    "end":"."
  },
  "visual_response": {
    "type": "list",
    "begin": "test",
    "item_template": "${load_result}"
  }
}
