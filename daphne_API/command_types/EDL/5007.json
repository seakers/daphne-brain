    { "name": "edl_metric_calculate", "type": "edl_metric_calculate", "options": "", "mandatory": true, "from_context":false}
  ],
"function":
{"run_template": "run_func.calculate_scorecard_metric('${current_mat_file}', '${edl_metric_calculate}', context)",
