{
  "type": "run_function",
  "params":
  [
    { "name": "current_scorecard", "type": "edl_mat_file", "options": "", "mandatory": true,"from_context":true}
  ],
"function":
{"run_template": "run_func.get_flag_summary('${current_scorecard}', context, 'flagged_results')",
    "result_type": "list",
    "result_fields": {
     "load_result": "item['command_result']"
    }
  },
  "voice_response": {
    "type": "list",
    "begin": "The flagged metrics in this scorecard are",
    "repeat": "${load_result}",
    "end":"."

  },
  "visual_response": {
    "type": "list",
    "begin": "The flagged metrics in this scorecard are",
    "item_template": "${load_result}"
  }
}